{
  "name": "pokedex",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch --bun dev.ts",
    "register": "bun run build -r && bun start",
    "start": "bun --bun .dressed",
    "build": "bun --bun dressed build",
    "checks": "tsc --noEmit && bun --bun biome check --write"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.4",
    "@types/bun": "latest",
    "@types/react": "^19.2.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@dressed/react": "^1.6.0",
    "dressed": "^1.10.1",
    "fuse.js": "^7.1.0",
    "pokenode-ts": "^1.20.0",
    "react": "^19.2.0"
  }
}
